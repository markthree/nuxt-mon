{
  "name": "nuxt-mon",
  "scripts": {
    "dev:theme": "pnpm --filter theme run dev",
    "build:theme": "pnpm --filter theme run build",
    "dev:web": "pnpm --filter web run dev",
    "build:web": "pnpm --filter web run build",
    "dev:h5": "pnpm --filter h5 run dev",
    "build:h5": "pnpm --filter h5 run build",
    "dev:admin": "pnpm --filter admin run dev",
    "build:admin": "pnpm --filter admin run build",
    "build:all": "pnpm --filter * run build"
  }
}
